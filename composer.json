{
    "name": "iosum/admin-auth",
    "description": "admin auth pkg",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Raj",
        "email": "rj.iosum@gmail.com"
    }],
    "require": {
        "php": "^7.3",
        "laravel/passport": "^10.0",
        "iosum/base": "dev-base"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Iosum\\AdminAuth\\": "src/",
            "Iosum\\AdminAuth\\Database\\Factories\\": "database/factories/",
            "Iosum\\AdminAuth\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iosum\\AdminAuth\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iosum\\AdminAuth\\Providers\\AdminAuthServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rjiosum/iosum.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
